.pc-header
  height 80px
  position fixed
  z-index 999
  top 0
  width 100%
  background-color #ffffff
  box-shadow: 0 0 5px #888
  justify-content center
  display flex
  .header-container
    max-width 1200px
    width 100%
    display flex
    justify-content space-between
    align-items center
    .logo-text
      font-size 36px
      font-weight bold
      font-family STSong
