@import '~@common/common'
.case-list-container
  width 100%
  height 100%
  .resume-search-wrap
    margin 20px
    min-height 100px
    background-color white
    border-radius 2px
    box-shadow 0 2px 4px rgba(0,21,41,.08)
    .search-title
      margin-top 20px
      height 30px
      line-height 30px
      padding-left 20px
      width 100px
      padding-right 20px
      font-weight 500
      border-radius 20px
      margin-left 20px
    .search-container
      flex-direction row
      display flex
      flex-wrap wrap
      .h-80
        height 80px
      .n-title
        white-space nowrap
        overflow hidden
        text-align center
        padding-right 10px
      .n-input
        max-width 200px
      .n-search
        width 200px
    .btns-container
      width 100%
      display flex
      justify-content flex-end
      flex-direction row
      height 60px
  .index-module-wrap
    margin 20px
    min-height calc(100% - 40px)
    background-color white
    border-radius 2px
    overflow hidden
    box-shadow 0 2px 4px rgba(0,21,41,.08)
    .prompt-text
      color orange
    .normal-text
      color text-color-666
      font-size 14px
      line-height 36px
      height 36px
    .table-wrap
      margin 20px
      .logo-img
        width 48px
        height 48px
    .pagination-wrap
      height 100px
      width 100%

.edit-modal-view
  .empty-img
    width 102px
    height 102px
    display block
    border 1px solid #eeeeee
    line-height 102px
    text-align center
  .img-uploader,.item-img
    width 102px
    height 102px
    display block
    // todo: css 集合处理
  .prompt-text
    color orange
  .remark-text
    font-size 12px
    line-height 12px
