@import '~@common/common'
.resume-tag-container
  width 100%
  height 100%
  .index-module-wrap
    margin 20px
    min-height calc(100%)
    background-color white
    border-radius 2px
    overflow hidden
    box-shadow 0 2px 4px rgba(0,21,41,.08)
    .prompt-text
      color orange
    .normal-text
      color text-color-666
      font-size 14px
      line-height 36px
      height 36px
    .table-wrap
      margin 20px
      .index-img
        width 100px
        height 75px
