{
  "name": "gy-nodejs-rcoco",
  "version": "1.0.0",
  "description": "react-admin-web",
  "private": true,
  "author": "goyoung",
  "scripts": {
    "clean": "rimraf ./build",
    "start": "npm run down && concurrently 'npm run dev'",
    "start-mock": "cross-env NODE_ENV=mock npm run down && concurrently 'npm run dev'",
    "dev": "react-app-rewired start",
    "build": "rimraf ./build & react-app-rewired build",
    "down": "node ./scripts/down/index.js",
    "local": "npm run mock & npm run start-mock",
    "mock": "json-server --watch ./mock/index.js --middlewares ./mock/middlewares.js --config mock/json-server.json",
    "eslint": "eslint . --ext .js,.jsx",
    "eslint-fix": "eslint . --ext .js,.jsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1",
    "@antv/g2": "^4.1.34",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "4.16.0",
    "axios": "^0.27.2",
    "babel-plugin-import": "^1.13.0",
    "chalk": "^4.1.2",
    "classnames": "^2.3.1",
    "concurrently": "4.1.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^0.9.1",
    "echarts": "^5.3.2",
    "inquirer": "^8.1.1",
    "js-cookie": "^3.0.1",
    "js-export-excel": "^1.1.4",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "moment": "^2.27.0",
    "rc-tween-one": "^3.0.3",
    "react": "^17.0.2",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-countup": "^6.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-error-overlay": "^6.0.9",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "wangeditor": "^4.7.9",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "async-mkdirp": "^1.2.7",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.19.0",
    "fs-extra": "^10.0.0",
    "json-server": "^0.17.0",
    "request": "^2.88.2",
    "url-parse": "^1.5.3"
  }
}
